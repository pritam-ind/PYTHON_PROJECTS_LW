   "source": [
    "import pyttsx3\n",
    "import time\n",
    "names = [\"Pritam\", \"Amit\", \"Mandeep\", \"Aashik\", \"Pradeep\"]\n",
    "\n",
    "for i in names:\n",
    "    speaker = pyttsx3.init()\n",
    "    speaker.say(f\"Hello {i} sir, How are you today?\")\n",
    "    speaker.runAndWait()\n",
    "    time.sleep(0.3)\n",
    "    del speaker\n",
    "\n"
   ]
